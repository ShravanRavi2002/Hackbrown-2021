{
  "name": "bear",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Bear"
  },
  "web_accessible_resources": [
    "Menu/torchys_nutrition_information.json",
    "Menu/torchys_logo.png"
  ]
}



